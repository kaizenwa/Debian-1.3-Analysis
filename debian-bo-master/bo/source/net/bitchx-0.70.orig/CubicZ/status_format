# Alternate STATUS_FORMAT* (status bar) settings for BitchX
# by BlackJac

SET STATUS_CHANNEL ] [%C
SET STATUS_CHANOP @
SET STATUS_CLOCK %T
SET STATUS_FORMAT [%T] [%@%N%#%A%C%+%W%M] %Q %H%B
SET STATUS_FORMAT1 [%T] [%@%N%#%A%C%+%W%M] %Q %H%B
SET STATUS_FORMAT2 %L%!%K Aw%^ %>%D
SET STATUS_LAG [Lag %L] 
SET STATUS_MAIL ] [Mail: %M
SET STATUS_MODE  (+%+)
SET STATUS_OPER_KILLS [nk %K|ok %K]
SET STATUS_UMODE  (+%#)
SET STATUS_USERS [O:%! N:%! I:%! V:%! F:%!] 
