alias do_varcase {
	@ blah = []
	fec ($*) x
	{
		if (rand(10) >= 5)
		{
			@ blah #= toupper($x)
		}
		{
			@ blah #= tolower($x)
		}
	}
	return $blah
}
alias varcase eval / $do_varcase($*)
alias varcasemsg eval /msg $0 $do_varcase($1-)
