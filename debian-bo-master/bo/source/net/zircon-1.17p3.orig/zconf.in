# ************ Use the configuration system to change this file ****
#
set obeah(program) {zircon}
set obeah(cfFile) {zconf.in}
set obeah(cfProg) {installZircon}
set obeah(makefile) {zircon.mk}
set obeah(creator) {1.4}
set obeah(forder) {f2 f0 f3 f1 f4}
set obeah(findex) 5
set obeah(iorder) {}
set obeah(iindex) 0
#
set config(f0) {/usr/local/lib/zircon}
set config(f1) {/usr/local/bin/wish4.1}
set config(f2) {Solaris}
set config(f3) {/usr/local/bin}
set config(f4) {gcc}
#
set label(f0) {Zircon Library Directory}
set label(f1) {tcl/tk Interpreter}
set label(f2) {System Type}
set label(f3) {Binary Directory}
set label(f4) {C Compiler}
#
set help(f0) {The directory where the zircon support files will be installed.}
set help(f1) {The full pathname of your tcl/tk interpreter.}
set help(f2) {The operating system you are running under. Valid values are : AIX, Alpha, HP-UX, Linux, SGI, Solaris or SunOS}
set help(f3) {The directory where the zircon program will be installed.}
set help(f4) {The name of your C compiler.}
#
set check(f0) {ob_dirCheck}
set check(f1) {ob_interpCheck {
	{{info tclversion} {7\.[45]} {This is neither tcl7.5 nor tcl7.4.}}
	{{info exists tk_version} {1} {This interpreter does not include tk.}}
	{{set tk_version} {4.[01]} {This interpreter does not include tk 4.1 or tk4.0.}}
	{{expr {[info tclversion] == {7.5} || [info exists dp_version]
	  || ([info exists tclx_library] && [infox have_sockets])}} {1} 
	  {Your tcl interpreter does not have networking support.}}
    }
}
set check(f2) {ob_valCheck {System type} {AIX Linux Alpha HP-UX SGI SunOS Solaris}}
set check(f3) {ob_dirCheck}
set check(f4) {}
#
set makeVar(f0) {LIB}
set makeVar(f1) {TCL}
set makeVar(f2) {SYS}
set makeVar(f3) {BINDIR}
set makeVar(f4) {CC}
#
#
