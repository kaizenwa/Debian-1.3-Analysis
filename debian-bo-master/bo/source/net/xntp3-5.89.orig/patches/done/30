# ADD CPP /nologo /MTd /W3 /Gm /GX /Zi /Od /I "..\include" /D "_DEBUG" /D "DEBUG" /D "WIN32" /D "_CONSOLE" /D "SYS_WINNT" /D "DES" /D "MD5" /D "NTP_LITTLE_ENDIAN" /D "SYSLOG_FILE" /FR /YX /c
