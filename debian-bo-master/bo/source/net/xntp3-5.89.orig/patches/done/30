!IF "$(CFG)" != "libntp - Win32 Release" && "$(CFG)" != "libntp - Win32 Debug"
!MESSAGE Invalid configuration "$(CFG)" specified.
!MESSAGE You can specify a configuration when running NMAKE on this makefile
