#! /bin/sh
#
# $Id: ypxfr_1perhour.in,v 1.1.1.1 1996/08/11 17:35:14 kukuk Exp $
#
# ypxfr_1perhour - Do hourly NIS map check/updates, to see, if we
#                 have missed one update.
#

YPBINDIR=@YPBINDIR@

for map in passwd.byname passwd.bygid shadow.byname publickey.byname
do
  $YPBINDIR/ypxfr $map
done
