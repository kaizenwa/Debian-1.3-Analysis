
#**************** OPTIONS *******************************

# Directory where the configuration and font files are to be placed
.PHONY: all clean install distclean

all clean install:

distclean:
	rm -f Makefile

ci:
	for i in *; do \
		if [ -f $$i ]; then \
			ci $(CI) -l -mUpdate -t-Initial $$i; \
		fi; \
	done;
