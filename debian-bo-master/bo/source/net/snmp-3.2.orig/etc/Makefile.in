
FILES = snmpd.conf snmpd.agentinfo

all:
	@/bin/true

install:
	@for f in $(FILES) ; do \
	  if [ ! -f /etc/$$f ] ; then \
		echo cp $$f /etc ; \
		cp $$f /etc ; \
	  else \
		echo /etc/$$f exists - not copied. ; \
          fi ; \
	done
	@echo "**"
	@echo "** Please Note: You may wish to run etc/installconf [-mini] password"
	@echo "**              to install your V2usec useable set."
	@echo "**"

clean:
	@/bin/true
