#!/bin/sh
# This is a simple filter for PCL language printers, i.e., most
# HP DeskJet printers (see /etc/printcap).

# reset the printer to its defaults
echo -ne \\033\E

# to avoid stair-stepping, convert "LF" to "CR+LF"
echo -ne \\033\&k2G

# print text
cat

# form feed
echo -ne \\f

# reset the printer on exit
echo -ne \\033\E
