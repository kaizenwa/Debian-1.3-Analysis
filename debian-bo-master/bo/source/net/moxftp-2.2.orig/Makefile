 PROGRAM = $(PNAME)

all:: $(PNAME)

$(PNAME): $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

saber_$(PNAME):: $(SRCS)
	# load $(ALLDEFINES) $(SRCS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

osaber_$(PNAME):: $(OBJS)
	# load $(ALLDEFINES) $(OBJS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

install:: $(PNAME)
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  $(PNAME) $(DESTDIR)$(BINDIR)

install.man:: $(PNAME).man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) $(PNAME).man $(DESTDIR)$(MANDIR)/$(PNAME).$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

install:: $(ANAME).ad
	@if [ -d $(DESTDIR)$(XAPPLOADDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)); fi
	$(INSTALL) -c $(INSTAPPFLAGS) $(ANAME).ad $(DESTDIR)$(XAPPLOADDIR)/$(ANAME)

#
# Some extra dependencies.
#
fb_rs.c:  Xftp.ad Mftp.ad Oftp.ad help
	perl <m_f.pl

help: help.man
	perl <fix_help.pl

Mftp.ad: Mftp.ad.nohelp help
	cat Mftp.ad.nohelp help >Mftp.ad

Xftp.ad: Xftp.ad.nohelp help
	cat Xftp.ad.nohelp help >Xftp.ad

Oftp.ad: Oftp.ad.nohelp help
	cat Oftp.ad.nohelp help >Oftp.ad

tran_table.c: dotxftp
	perl <m_t.pl

World::
	$(MAKE) Makefiles
	$(MAKE) clean
	$(MAKE) includes
	$(MAKE) all

uninstall:
	-rm $(DESTDIR)$(XAPPLOADDIR)/$(ANAME)
	-rm  $(DESTDIR)$(BINDIR)/$(PNAME)

