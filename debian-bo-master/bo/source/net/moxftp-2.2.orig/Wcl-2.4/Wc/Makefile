       LINTLIBS = $(LINTXLIB)

# # Wcl Library Template for X11R5
# #

      LIBRARY_g = $(LIBRARY_NAME)$(DLX)
      LIBRARY_p = $(LIBRARY_NAME)_p

all::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

includes::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

.c.o:
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(RM) $@
	$(CC) -c $(CFLAGS) $(_NOOP_) $*.c

clean::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

all:: lib$(LIBRARY_NAME).a

lib$(LIBRARY_NAME).a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)

install:: lib$(LIBRARY_NAME).a
	@if [ -d $(DESTDIR)$(USRLIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(USRLIBDIR)); fi
	$(INSTALL) -c $(INSTLIBFLAGS) lib$(LIBRARY_NAME).a $(DESTDIR)$(USRLIBDIR)

lintlib:: llib-l$(LIBRARY_NAME).ln

llib-l$(LIBRARY_NAME).ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)$(LIBRARY_NAME) $(LINTFLAGS) $(SRCS)

install.ln:: llib-l$(LIBRARY_NAME).ln
	@if [ -d $(DESTDIR)$(LINTLIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINTLIBDIR)); fi
	$(INSTALL) -c $(INSTLIBFLAGS) llib-l$(LIBRARY_NAME).ln $(DESTDIR)$(LINTLIBDIR)

includes::
	@if [ -d $(BUILDINCDIR)/$(LIBRARY_NAME) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(BUILDINCDIR)/$(LIBRARY_NAME)); fi
	@(set -x; cd $(BUILDINCDIR)/$(LIBRARY_NAME); for i in $(HEADERS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)/../$(CURRENT_DIR)/$$i .; \
	done)

install:: $(HEADERS)
	@if [ -d $(DESTDIR)$(INCDIR)/$(LIBRARY_NAME) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)/$(LIBRARY_NAME)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(DESTDIR)$(INCDIR)/$(LIBRARY_NAME)); \
	done

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

