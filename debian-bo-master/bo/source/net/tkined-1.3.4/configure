  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$TKINED" in
  /*)
  ac_cv_path_TKINED="$TKINED" # Let the user override the test with a path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS="${IFS}:"
  for ac_dir in $PATH; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_TKINED="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
esac
fi
TKINED="$ac_cv_path_TKINED"
if test -n "$TKINED"; then
  echo "$ac_t""$TKINED" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

  if test -n "$ac_cv_path_TKINED"; then
    prefix=`echo $ac_cv_path_TKINED|sed 's%/[^/][^/]*//*[^/][^/]*$%%'`
  fi
fi


