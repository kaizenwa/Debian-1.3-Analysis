if eval "test \"`echo '$''{'ac_cv_func_Tcl_ValidateAllMemory'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  cat > conftest.$ac_ext <<EOF
#line 982 "configure"
#include "confdefs.h"
/* System header to define __stub macros and hopefully few prototypes,
    which can conflict with char Tcl_ValidateAllMemory(); below.  */
#include <assert.h>
/* Override any gcc2 internal prototype to avoid an error.  */
char Tcl_ValidateAllMemory(); 

int main() { return 0; }
int t() {

/* The GNU C library defines this for functions which it implements
    to always fail with ENOSYS.  Some functions are actually named
    something starting with __ and the normal name is an alias.  */
#if defined (__stub_Tcl_ValidateAllMemory) || defined (__stub___Tcl_ValidateAllMemory)
choke me
#else
Tcl_ValidateAllMemory();
#endif

; return 0; }
EOF
if eval $ac_link; then
  rm -rf conftest*
  eval "ac_cv_func_Tcl_ValidateAllMemory=yes"
else
  rm -rf conftest*
  eval "ac_cv_func_Tcl_ValidateAllMemory=no"
fi
rm -f conftest*

fi
