#
#	most - a SunOS-4 install w/*everything* under /local
#
CPPFLAGS="-I. -I.. -I../src -I/local/include"
CFLAGS="-g"
LDFLAGS="-g -L/local/lib"
LIBS="-lident"
USE_SYMLINKS=yes
OS_TYPE=sun_os4.1
LMAIL=/usr/bin/lmail
MISC_DEFINES='SMAIL_LOG_STYLE=2:NO_FORWARDTO_FILE:USE_LSEARCH_REGEXCMP:USE_TARGET_DOMAIN'
HAVE=RFC1413:EHLO
SMAIL_BIN_DIR=/local/bin
SMAIL_NAME=/usr/lib/sendmail
LIB_DIR=/local/etc/smail
UTIL_BIN_DIR=/local/libexec/smail
NEWALIASES=/local/sbin/newaliases
SPOOL_DIRS=/local/var/spool/smail
LOG_DIR=/local/var/log/smail
UNSHAR_MAP_DIR=/local/var/spool/uumaps
NEWS_SPOOL_DIR=/local/var/spool/news
UUWHO_FILE=/local/var/spool/smail/uuwho
TMP_DIR=/local/var/spool/smail/tmp
MAN1=/local/share/man/man1
MAN1_EXT=1
MAN5=/local/share/man/man5
MAN5_EXT=5
MAN8=/local/share/man/man8
MAN8_EXT=8
PATHS_FILE=/local/var/spool/smail/paths
PATHS_TYPE=bsearch
FORCE_PATHS_FILE=forcepaths
FORCE_PATHS_TYPE=bsearch
FORCE_SMTP_FILE=forcesmtp
FORCE_SMTP_TYPE=bsearch
ALIASES_FILE=/local/etc/aliases
ALIASES_TYPE=dbm
MISC_C_DEFINES='
#define NO_ALIASES_OPTIONAL		/* aliases file is not optional */
#define ALIASES_TRYAGAIN		/* defer message if no aliases file */
'
