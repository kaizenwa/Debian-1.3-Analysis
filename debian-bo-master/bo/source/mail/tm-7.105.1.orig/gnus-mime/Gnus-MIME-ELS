;;; -*-Emacs-Lisp-*-
;;;
;;; $Id: Gnus-MIME-ELS,v 0.2 1996/09/04 14:07:16 morioka Exp $
;;;

(setq gnus-mime-modules-to-compile
      '(gnus-mime gnus-charset gnus-art-mime gnus-sum-mime message-mime))

(or (module-installed-p 'gnus-load)
    (setq gnus-mime-modules-to-compile
	  (cons 'gnus-mime-old
		gnus-mime-modules-to-compile))
    )

(setq gnus-mime-modules gnus-mime-modules-to-compile)

;;; Gnus-MIME-ELS ends here
