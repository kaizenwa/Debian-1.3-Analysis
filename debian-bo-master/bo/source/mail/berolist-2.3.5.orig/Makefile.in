CC = @CC@
CFLAGS = @CFLAGS@ @LIBS@ @DEFS@
all: list @DO_CGI@
list: tool.c socket.c list.c
cgi: messages display
messages: tool.c cgitool.c messages.c
display: tool.c cgitool.c display.c
realclean: distclean
distclean: clean
	@rm -f config.log
	@rm -f config.cache
	@rm -f config.status
	@rm -f confdefs.h
	@rm -f Makefile
clean:
	@rm -f *.o
	@rm -f *~
	@rm -f list
	@rm -f messages
	@rm -f display
