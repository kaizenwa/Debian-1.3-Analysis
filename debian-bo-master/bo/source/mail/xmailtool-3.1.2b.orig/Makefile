       COMPDIR = compface

HAS_XFACE     = -DHAS_XFACE

XFACE_SRC = xface.c
XFACE_OBJ = xface.o

        SUBDIRS = $(COMPDIR)

all::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "making" all "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS)  all); \
	done

LOCAL_LIBRARIES = $(XAWLIB) $(XMULIBONLY) $(XTOOLLIB) $(XLIB)
        DEPLIBS = $(DEPXAWLIB) $(DEPXMULIB) $(DEPXTOOLLIB) $(DEPXLIB)

  SYS_LIBRARIES = -lm compface/libcompface.a

           SRCS = check_mail.c xmailtool.c proc.c util.c mail_int.c 		date.c $(XFACE_SRC) expand_value.c get_UV_value.c 		mailhandler.c mailrc.c setaliases.c setvars.c uname.c

      TOOL_SRCS = check_mail.c xmailtool.c proc.c util.c mail_int.c 		setvars.c aliases.c $(XFACE_SRC) mailhandler.c                 expand_value.c get_UV_value.c setaliases.c

           OBJS = check_mail.o xmailtool.o proc.o util.o mail_int.o 		date.o $(XFACE_OBJ) expand_value.o get_UV_value.o 		mailhandler.o mailrc.o setaliases.o setvars.o uname.o

        PROGRAM = xmailtool
        MANPAGE = $(PROGRAM).man
  INCLUDE_FILES = defs.h patchlevel.h

       HELP_DIR = $(LIBDIR)/xmailtool
   DEF_HELPFILE = -DDEFAULT_HELPFILE=\"$(HELP_DIR)/xmailtool.help\"

        MBOXDIR = -DMAILBOX_DIRECTORY=\"/varr/spool/mail\"

        MSENDER = -DUSE_SENDMAIL

       SENDMAIL = -DMSENDER=\"/usr/lib/sendmail\"

      PRINTER_CMD =

      HYPERTEXT = -DHYPERTEXT=\"netscape\"

