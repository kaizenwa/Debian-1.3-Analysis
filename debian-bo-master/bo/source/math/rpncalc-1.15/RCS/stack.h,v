struct elm
{
  double value;
  struct elm *prev, *next;
};

double push(double elem);
double pop(void);
double pick(double op1);
double drop(void);
double dupel(double d);
double dupel2(double d);
double dupn(double n, double d);
double dropn(double n);
double clear(void);
double over(void);
double depth(void);
double swap(void);
double roll(void);
double showstack(void);
double sethex(void);
double setdec(void);
double setoct(void);
double setchar(void);
@


