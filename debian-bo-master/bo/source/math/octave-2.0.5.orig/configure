### Do special things for g++.

ADD_CXX_WALL=false
NO_IMPLICIT_TEMPLATES=
gxx_version=`$CXX -v 2>&1 | grep "^.*g.. version" | \
  sed -e 's/^.*g.. version *//' -e 's/cygnus-//'`
case "$gxx_version" in
  2.[6789].*|2.[123456789][0123456789].*)
    if test -z "$EXTERN_CXXFLAGS"; then
      ADD_CXX_WALL=true
    fi
    NO_IMPLICIT_TEMPLATES="-fno-implicit-templates"
    echo "$ac_t""defining NO_IMPLICIT_TEMPLATES to be $NO_IMPLICIT_TEMPLATES" 1>&6
  ;;
  1.*|2.[012345].*)
    { echo "configure: error: g++ version $gxx_version will not work to compile Octave" 1>&2; exit 1; }
  ;;
  *)
    echo "configure: warning: Octave has only been tested with g++, and I can't find it" 1>&2
  ;;
esac

HOST_CXXFLAGS=
case "$gxx_version" in
  2.6.*)
    case "$canonical_host_type" in
      rs6000-ibm-aix* | powerpc-ibm-aix*)
        HOST_CXXFLAGS="-Wa,-u"
        echo "$ac_t""defining HOST_CXXFLAGS to be $HOST_CXXFLAGS" 1>&6
      ;;
    esac
  ;;
esac

