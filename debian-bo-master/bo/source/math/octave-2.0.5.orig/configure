  enableval="$enable_bounds_check"
  if test "$enableval" = no; then BOUNDS_CHECKING=false; fi
fi

if $BOUNDS_CHECKING; then
  cat >> confdefs.h <<\EOF
#define BOUNDS_CHECKING 1
EOF

