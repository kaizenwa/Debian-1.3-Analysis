
/* Imakefile to built yabasic, including X-grafics     */
/* This file is used by xmkmf to generate a Makefile */

/* use gcc, might be changed on some systems:        */
              CC = gcc -Wall

/* INCROOT and USRLIBDIR are supplied by xmkmf:      */
       LDOPTIONS = -I$(INCROOT) -L$(USRLIBDIR)

/* Include  X-grafic:                                */
EXTRA_LOAD_FLAGS = -DUNIX_X11

/* generate Makefile:                                */
NormalProgramTarget(yabasic,yabasic.c flex.c bison.c,Makefile,-lm,-lX11)
