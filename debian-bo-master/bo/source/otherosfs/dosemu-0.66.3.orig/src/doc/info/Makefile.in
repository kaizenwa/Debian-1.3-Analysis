@SET_MAKE@

all:
	-$(MAKE) dosemu.info
	-$(MAKE) dosemu.dvi

dvi: dosemu.dvi

info: dosemu.info

dosemu.dvi: dosemu.texinfo
	tex dosemu.texinfo
	texindex dosemu.??
	tex dosemu.texinfo

dosemu.info: dosemu.texinfo
