      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
esac
fi
HOSTNAME="$ac_cv_path_HOSTNAME"
if test -n "$HOSTNAME"; then
  echo "$ac_t""$HOSTNAME" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

