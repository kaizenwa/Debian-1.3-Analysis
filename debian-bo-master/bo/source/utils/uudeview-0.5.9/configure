if test -f /etc/resolv.conf ; then
	#
	# note: Parameter for tr is <space><tab>
	#
	domainname=`grep '^domain' /etc/resolv.conf | tr -d ' 	' | sed 's/domain//'`
fi
fi

#
# on some systems, hostname returns the FQN
#

if test "x$domainname" = "x" ; then
if test "x$HOSTNAME" != "x" ; then
	if test "x`$HOSTNAME | grep "\\."`" != "x" ; then
		domainname=`$HOSTNAME | sed 's/[a-zA-Z0-9]*\.//'`
	fi
fi
fi

if test "x$domainname" = "x" ; then
	echo "$ac_t""unknown" 1>&6
	echo "configure: warning: Couldn't figure out your domain name. Configuring bogus" 1>&2
	echo "configure: warning: domain. If you intend to post directly from uuenview," 1>&2
	echo "configure: warning: rerun configure and use --enable-domain=your-domainname" 1>&2
	echo "configure: warning: with your real domain name." 1>&2
	domainname="i.dont.know.where.i.am"
else
	echo "$ac_t""$domainname" 1>&6
fi

cat >> confdefs.h <<EOF
#define DOMAINNAME "$domainname"
EOF


