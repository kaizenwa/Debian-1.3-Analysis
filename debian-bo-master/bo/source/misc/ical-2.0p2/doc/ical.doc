proc html_insert {w tag text} {
    set x [$w index insert]
    $w insert insert $text
    set y [$w index insert]
    foreach t [$w tag names $x] {
	$w tag remove $t $x $y
    }
    $w tag add $tag $x $y
}

proc html_goto {w label} {
    $w yview label_$label
    catch {$w mark unset anchor}
}

proc html_title {w title} {
    wm title [winfo toplevel $w] $title
}

