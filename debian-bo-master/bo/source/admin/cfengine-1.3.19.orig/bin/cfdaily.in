#!/bin/csh -f
###########################################
#
# Daily configuration check
#
###########################################
 
setenv CFINPUTS @prefix@/lib/cfengine/inputs

set path = ( /bin /usr/bin /usr/sbin @prefix@/bin )

if ( -f /etc/cfengine.pid ) then

   rm -f /etc/cfengine.pid

endif

@prefix@/bin/cfengine

