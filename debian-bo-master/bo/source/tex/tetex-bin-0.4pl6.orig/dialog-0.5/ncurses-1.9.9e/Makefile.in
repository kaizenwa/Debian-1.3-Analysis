# Master Makefile for ncurses library.

SHELL = /bin/sh

NC_MFLAGS = @nc_cv_makeflags@

@SET_MAKE@

DIRS_TO_MAKE = @DIRS_TO_MAKE@

all ::	$(DIRS_TO_MAKE)

$(DIRS_TO_MAKE) :
	mkdir $@

# Put the common rules here so that we can easily construct the list of
# directories to visit.
all \
clean \
distclean \
mostlyclean \
realclean \
install ::
