print.align.key        = 18
print.all.strings      = on
print.braces           = on
print.indent           = 2
print.line.length      = 77
print.newline          = 1
print.parentheses      = off
print.use.tab          = on
quiet                  = off
rewrite.case.sensitive = on
rewrite.limit          = 512
select.case.sensitive  = off
select.fields          = "$key"
