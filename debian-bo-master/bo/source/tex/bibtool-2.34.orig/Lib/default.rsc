ignored.word           = "the"
ignored.word           = "le"
ignored.word           = "les"
ignored.word           = "la"
ignored.word           = "un"
ignored.word           = "une"
ignored.word           = "el"
ignored.word           = "il"
ignored.word           = "der"
ignored.word           = "die"
ignored.word           = "das"
ignored.word           = "ein"
ignored.word           = "eine"
key.base               = lower
key.expand.macros      = on
key.format             = short
key.generation         = off
key.number.separator   = "*"
new.entry.type         = "Article"      
