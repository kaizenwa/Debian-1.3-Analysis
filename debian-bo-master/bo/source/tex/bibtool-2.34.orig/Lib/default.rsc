expand.macros          = on
fmt.et.al              = ".ea"
fmt.inter.name         = "-"
fmt.name.name          = "."
fmt.name.pre           = "."
fmt.name.title         = ":"
fmt.title.title        = "-"
ignored.word           = "a"
