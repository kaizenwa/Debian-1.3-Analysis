:- module getopt.
:- interface.
:- import_module int, string, bool, list, map, std_util.
:- type (getopt:option_ops(OptionType)).
:- inst ((getopt:option_ops)) = bound(option_ops((pred(in, out) is semidet), (pred(in, out) is semidet), (pred(out, out) is nondet)) ; option_ops((pred(in, out) is semidet), (pred(in, out) is semidet), (pred(out, out) is nondet), (pred(in, in, in, out) is semidet))).
:- type (getopt:option_data).
:- type (getopt:special_data).
:- type (getopt:option_table(OptionType)) == map(OptionType, (getopt:option_data)).
:- type (getopt:maybe_option_table(OptionType)).
