#! /bin/sh

# Test to make sure name canonicalization happens for texinfo.

. $srcdir/defs || exit 1

cat > Makefile.am << 'END'
info_TEXINFOS = zar-doz.texi
zar_doz_TEXINFOS = frob.texi
END

: > texinfo.tex
: > zar-doz.texi
: > frob.texi

$AUTOMAKE || exit 1

grep zar-doz_TEXINFOS Makefile.in && exit 1
exit 0
