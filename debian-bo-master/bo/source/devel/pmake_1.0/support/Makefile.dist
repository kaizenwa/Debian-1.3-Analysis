# A Not so Simple Makefile

# Define the ar command
AR= ar
ARFLAGS= rv

# Define the Ranlib command
RANLIB= ranlib

# What OBJS are needed for pmake
OBJS=	setenv.o getenv.o strerror.o

all: support.a

support.a: ${OBJS}
	${AR} ${ARFLAGS} $@ ${OBJS}
	${RANLIB} $@

clean:
	rm -f support.a ${OBJS}

# From here down are the new dependencies
setenv.o : setenv.c
getenv.o : getenv.c
strerror.o : strerror.c

