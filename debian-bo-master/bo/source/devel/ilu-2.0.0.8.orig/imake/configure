    echo -n "*** Where are the Tcl include files?  "
    read TK_INCLUDE_2
    TK_INCLUDES="-I$x_includes -I$TK_INCLUDE_1 -I$TK_INCLUDE_2"
    echo "$ac_t""tk includes are $TK_INCLUDES" 1>&6
  fi
