#define IHaveSubdirs

SUBDIRS	= utl_tmpl

LOCALINCLUDES = -I../include -D$(IDL_CFE_OSV)

LIB	= libutil.a

SRCS	= \
	utl_decllist.cc \
	utl_error.cc \
	utl_exceptlist.cc \
	utl_exprlist.cc \
	utl_global.cc \
	utl_identifier.cc \
	utl_idlist.cc \
	utl_indenter.cc \
	utl_labellist.cc \
	utl_list.cc \
	utl_namelist.cc \
	utl_scope.cc \
	utl_stack.cc \
	utl_string.cc \
	utl_strlist.cc

OBJS	= $(SRCS:.cc=.o)

libs	: $(LIB)

NormalObjectRule()
LibraryTarget($(LIB), $(OBJS))
