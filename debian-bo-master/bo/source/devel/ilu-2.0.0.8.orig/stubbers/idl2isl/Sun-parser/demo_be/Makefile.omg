include	../idl_make_vars

BE_FILES	= \
		be_generator.cc \
		be_classes.cc \
		be_init.cc \
		be_produce.cc \
		be_args.cc

BE_HEADERS 	= \
		be.hh \
		be_generator.hh \
		be_classes.hh

BE_OBJS		= $(BE_FILES:.cc=.o)

BE_LIB_NAME	= libbe.a

all: $(BE_LIB_NAME)

$(BE_OBJS): $(BE_HEADERS)

$(BE_LIB_NAME): $(BE_OBJS)
	$(AR) $(ARFLAGS) $@ $(BE_OBJS)
	$(RANLIB) $@

clean:
	/bin/rm -f $(BE_OBJS) $(BE_LIB_NAME)
