LOCALINCLUDES = -I../Sun-parser/include

LIB	= libbe.a

HDRS 	= \
	be.hh \
	be_globals.hh \
	be_generator.hh \
	be_classes.hh

SRCS	= \
	be_generator.cc \
	be_classes.cc \
	be_gen_isl.cc \
	be_version.cc \
	be_init.cc \
	be_produce.cc \
	be_args.cc

OBJS	= $(SRCS:.cc=.o)

libs	: $(LIB)

$(OBJS) : $(HDRS)

NormalObjectRule()
LibraryTarget($(LIB), $(OBJS))
