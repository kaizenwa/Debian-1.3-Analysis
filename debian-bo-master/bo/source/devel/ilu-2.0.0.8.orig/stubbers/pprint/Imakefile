LOCALINCLUDES = -I../parser

CDEBUGFLAGS = -g

OBJS =  pprint.o \
	names.o

NormalObjectRule()

ProgramTarget(islpp, $(OBJS), ../parser/libilu-parser.a,)

InstallProgram(islpp, $(DESTDIR)/bin)
InstallRO(islpp.1, $(DESTDIR)/man/man1)


