#define IHaveSubdirs

#ifdef ADD_COMMONLISP_LANGUAGE
 LISPDIR = lisp
#endif /* ADD_COMMONLISP_LANGUAGE */

#ifdef ADD_C_LANGUAGE
 CDIR = c
#endif

#ifdef ADD_CPLUSPLUS_LANGUAGE
 CPLUSPLUSDIR = cpp
#endif

#ifdef ADD_MODULA3_LANGUAGE
 MOD3DIR = m3
#endif

#ifdef ADD_PYTHON_LANGUAGE
 PYTHONDIR = python
#endif

#ifdef ADD_JAVA_LANGUAGE
 JAVADIR = java
#endif

#ifdef ADD_IDL_SUPPORT
 IDL2ISLDIR = idl2isl
#endif

SUBDIRS = parser pprint $(CPLUSPLUSDIR) $(CDIR) $(LISPDIR) $(MOD3DIR) $(IDL2ISLDIR) $(PYTHONDIR) $(JAVADIR)

