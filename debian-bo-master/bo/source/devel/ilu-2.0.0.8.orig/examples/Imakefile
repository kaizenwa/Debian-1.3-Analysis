#define IHaveSubdirs

#if (defined(ADD_C_LANGUAGE) && defined(ADD_PYTHON_LANGUAGE) && defined(PYTHON_LIBRARIES_INSTALLED))
 MULTLANG_DIR = multlang
#endif

#if (defined(ADD_C_LANGUAGE) || defined(ADD_PYTHON_LANGUAGE))
 TUT_DIRS = tutorial
#endif

#ifdef ADD_C_LANGUAGE
 C_DIRS = test2 timeit changeup dialog objtable
#endif

#ifdef ADD_PYTHON_LANGUAGE
 PYTHON_DIRS = blob
#endif

#if (defined(ADD_PYTHON_LANGUAGE) && defined(ADD_IIOP_PROTOCOL))
 PYTHON_IIOP_DIR = cubit black-widow-bank
#endif

#if (defined(ADD_PYTHON_LANGUAGE) && defined(ADD_IIOP_PROTOCOL) && defined(ADD_IDL_SUPPORT))
 IDL2ISL_PYTHON_IIOP_DIR = orbix-grid
#endif

#if (defined(ADD_C_LANGUAGE) && defined(ADD_IIOP_PROTOCOL))
 IIOPTEST1_DIR = iioptest1
#endif

#if (defined(ADD_C_LANGUAGE) && defined(ADD_HTTP_PROTOCOL))
 HTTEST_DIR = httest
#endif

#ifdef ADD_COMMONLISP_LANGUAGE
 LISP_DIRS = fs
#endif

#ifdef ADD_MODULA3_LANGUAGE
 M3_DIRS = foogen foo
#endif

SUBDIRS = test1 $(M3_DIRS) $(LISP_DIRS) $(CPP_DIRS) $(PYTHON_DIRS) $(C_DIRS) $(MULTLANG_DIR) $(TUT_DIRS) $(PYTHON_IIOP_DIR) $(IDL2ISL_PYTHON_IIOP_DIR) $(HTTEST_DIR)


