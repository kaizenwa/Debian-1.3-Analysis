MkdirTarget($(DESTDIR)/examples)
MkdirTarget($(DESTDIR)/examples/test2)

InstallRO(Fibber.isl, $(DESTDIR)/examples/test2)
InstallRO(Batcher.isl, $(DESTDIR)/examples/test2)
InstallRO(fprog.c, $(DESTDIR)/examples/test2)
InstallRO(bsvr.c bclnt.c, $(DESTDIR)/examples/test2)
InstallRO(README, $(DESTDIR)/examples/test2)
InstallFileToFile(runImakefile, $(INSTINCFLAGS), $(DESTDIR)/examples/test2/Imakefile)


