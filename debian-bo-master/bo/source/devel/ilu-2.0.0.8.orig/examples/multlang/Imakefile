/* Last edited by Mike Spreitzer March 13, 1996 11:01 am PST */

runImakefile : runImakefile.dist
	$(RM) python-libs
	$(PYTHON_EXEC_PREFIX)/bin/python figurePythonLibs $(PYTHON_EXEC_PREFIX)/lib/python/lib/Makefile >python-libs
	$(SED) -e "s;PYTHON_SYSTEM_LIBS;`cat python-libs`;" <runImakefile.dist >runImakefile

MkdirTarget($(DESTDIR)/examples)
MkdirTarget($(DESTDIR)/examples/multlang)

InstallRO(multlang.isl multlang.c multlangimpl.py README, $(DESTDIR)/examples/multlang)

InstallFileToFile(runImakefile, $(INSTINCFLAGS), $(DESTDIR)/examples/multlang/Imakefile)

CleanTarget(runImakefile python-libs)
