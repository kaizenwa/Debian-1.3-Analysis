dnl Process this file with autoconf to produce a configure script.
AC_PREREQ(2.5)
AC_INIT(patch.c)
AC_CONFIG_HEADER(config.h)
CC=${CC-cc}
AC_SUBST(CC)
AC_PROG_CPP
AC_AIX
AC_MINIX
AC_ISC_POSIX
AC_C_CONST
AC_PROG_INSTALL
AC_HEADER_DIRENT
if test -z "$ac_dir_header"; then
  AC_DEFINE(NODIR)
fi
AC_TYPE_SIGNAL
AC_HEADER_STDC
AC_CHECK_HEADERS(unistd.h string.h fcntl.h)
AC_REPLACE_FUNCS(rename)
AC_CHECK_FUNCS(pathconf strerror)
AC_FUNC_ALLOCA
AC_SYS_LONG_FILE_NAMES
AC_OUTPUT(Makefile)
