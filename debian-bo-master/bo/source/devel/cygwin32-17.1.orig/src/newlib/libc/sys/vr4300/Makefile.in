srcdir = .

SHELL = /bin/sh

TOP = ../../..
SRCTOP = ../../..
TARGETDOC = ../../targetdep.tex

OFILES = \
	syscalls.o \


#### Host, target, and site specific Makefile fragments come in here.
###

# We need the -DCLANGUAGE below because .h files are shared between
# asm and C, using only this flag to distinguish.

.c.o:
	$(CC) -DCLANGUAGE $(CFLAGS) $(CROSS_CFLAGS) $(TARGET_CFLAGS) $(NEWLIB_CFLAGS) -I$(srcdir)/include $(INCLUDES) -c $<

.S.o:
	$(CC) $(CFLAGS) $(CROSS_CFLAGS) $(TARGET_CFLAGS) $(NEWLIB_CFLAGS) -I$(srcdir)/include $(INCLUDES) -c $<


all: lib.a crt0.o
lib.a: $(OFILES)
	rm -f $@
	$(AR) $(AR_FLAGS) $@ $< $(OFILES)

doc:	

clean mostlyclean:
	$(RM) crt0.o *~

distclean maintainer-clean realclean: clean
	rm -f Makefile config.status

Makefile: Makefile.in $(host_makefile_frag) $(target_makefile_frag)
	$(SHELL) config.status

# to support SunOS VPATH

crt0.o:		crt0.S
