#  A simple translator to postfix form.
#
#
#      operator syntax
#
#      binary operators
#
<bop>(x,y,z)	"("	x	","	z	")<bop>"
<aop>(x,y,z)	"("	x	","	z	")<aop>"
<bcs>(x,y,z)	"("	x	","	z	")<bcs>"

#
#      unary operators
#
<uop>(x,y)	"("	y	")<uop>"
<ucs>(x,y)	"("	y	")<ucs>"

#
#      miscellaneous expressions
#
Apply(x,y,z)	"("	x	","	z	")!"
Brace(x,y,z)	"{\n"	y	"\n}"
Brack(x,y,z)	"["	y	"]"
Colon(x)	":"
Compound(x,y,z)	x	";\n"	z
Elst0(x)	x
Elst1(x,y,z)	x	","	z
Field(x,y,z)	"("	x	","	z	")."
Invoke(w,x,y,z)	"("	y	")"	w
Mcolon(x)	"-:"
Paren(x,y,z)	"("	y	")"
Pcolon(x)	"+:"
Pdco0(x,y,z)	x	"{"	"}"
Pdco1(w,x,y,z)	w	"{"	y	"}"
Pdcolist0(x)	x
Pdcolist1(x,y,z)	x	","	z
Progend(x,y)	""
Section(u,v,w,x,y,z)	u	"["	w	x	y	"]"
Subscript(w,x,y,z)	w	"["	y	"]"
