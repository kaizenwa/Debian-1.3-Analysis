../stamp-gnatlib:
	@if [ ! -f stamp-gnatlib ] ; then \
	  echo You must first build the GNAT library: make gnatlib; \
	  false; \
	else \
	  true; \
	fi

install-gnatlib: ../stamp-gnatlib
	rm -rf $(ADA_RTL_OBJ_DIR)
	-mkdir $(ADA_RTL_OBJ_DIR)
	(cd rts; \
	  for f in *$(objext) *.ali; do \
	    cp -p $$f $(ADA_RTL_OBJ_DIR)/$$f; \
	    chmod a-x $(ADA_RTL_OBJ_DIR)/$$f; \
	  done)
	(cd rts; \
	  for f in *.a ; do \
	    cp -p $$f $(libsubdir)/$$f; \
	    chmod a-x $(libsubdir)/$$f; \
	  done)
	rm -rf $(ADA_INCLUDE_DIR)
	-mkdir $(ADA_INCLUDE_DIR)
	(cd rts; \
	  for f in *.ads *.adb; do \
	    cp -p $$f $(ADA_INCLUDE_DIR)/$$f; \
	    chmod a-wx $(ADA_INCLUDE_DIR)/$$f;\
	  done)

../stamp-gnatlib2:
	rm -f rts/s-*.ali
	rm -f rts/s-*.o
	rm -f rts/a-*.ali
	rm -f rts/a-*.o
	rm -f rts/*.ali
	rm -f rts/*.o
	touch ../stamp-gnatlib2
	rm -f ../stamp-gnatlib

# new GNULL Interface;  (first branch of the case)
   #  1. copy 4xyyy.ad[sb] -->-- a-yyy.ad[sb] 
   #  2. copy 5xyyy.ad[sb] -->-- s-yyy.ad[sb] 
   #  3. copy 6xyyy.ad[sb] -->-- i-yyy.ad[sb] 
   #  4. copy 7zyyy.ad[sb] -->-- z-yyy.ad[sb] 
# old GNULL Interface;  (second branch of the case)
   #  1. copy 1xyyy.ad[sb] -->-- a-yyy.ad[sb] 
   #  2. copy 2xyyy.ad[sb] -->-- s-yyy.ad[sb] 
   #  3. copy 3xyyy.ad[sb] -->-- i-yyy.ad[sb] 

../stamp-gnatlib1: Makefile
	rm -rf rts
	mkdir rts
	chmod u+w rts
	(\
	   case $(target) in \
		sparc-sun-sunos4*) 	letter=u ;;\
		sparc-sun-solaris2* |\
		sparc-sun-sunos5*)	letter=s ;;\
	        *86*-linux*)		letter=l ;;\
	        mips-sgi-irix*)		letter=g ;;\
		hppa*-hp-hpux*)		letter=h ;;\
		*-*-rtems* | *-rtems)	letter=r ;;\
		*-go32-msdos | *-go32)	letter=d ;;\
	        alpha-dec-osf*)		letter=a ;;\
		ppc-mac-machten)	letter=m ;;\
		*-*-cygwin32*)		letter=w ;;\
		*)			letter=n ;;\
	   esac ; \
	   \
	   srcdir1=`cd $(srcdir); pwd`;\
	   ( \
	        targdir=`pwd`; \
		cd $$srcdir1; \
		for f in $(ADA_INCLUDE_SRCS) $(LIBGNAT_SRCS); do  \
	      	   ln -s $$srcdir1/$$f $$targdir/rts/$$f > /dev/null 2>&1 ||  \
	      	   cp -p $$srcdir1/$$f $$targdir/rts/$$f; \
	   	done; \
	   ) ; \
	   cd rts; \
	   case $(target) in \
		hppa*-hp-hpux*		|\
		mips-sgi-irix*		|\
		*-*-cygwin32*		|\
		alpha-dec-osf*)		\
		\
		   for f in $$srcdir1/[123]$$letter*.ad[sb]; do  \
		      g=`basename $$f | sed -e "s/^1$$letter/a-/" \
					    -e "s/^2$$letter/s-/" \
					    -e "s/^3$$letter/i-/"` ; \
		      rm -f $$g ; \
		      ln -s $$f $$g > /dev/null 2>&1 || cp -p $$f $$g ; \
		   done;; \
		\
		sparc-sun-sunos4*      |\
		sparc-sun-solaris2*    |\
		sparc-sun-solaris5*    |\
		ppc-mac-machten        |\
		*-*-rtems* | *-rtems*  |\
		*-go32-msdos | *-go32  |\
		*86*-linux*            |\
		*) \
		\
		   for f in  $(GNAT_RTL_OLD_OBJS); do \
		      g=`basename $$f | sed -e "s:\..*:.ad\[sb\]:" `; \
		      rm -f $$g ; \
		   done; \
		   \
		   for f in $$srcdir1/[456]$$letter*.ad[sb]; do  \
		      g=`basename $$f | sed -e "s/^4$$letter/a-/" \
					    -e "s/^5$$letter/s-/" \
					    -e "s/^6$$letter/i-/" ` ; \
		      rm -f $$g ; \
		      ln -s $$f $$g > /dev/null 2>&1 || cp -p $$f $$g ; \
		   done; \
		\
		   for f in $$srcdir1/7*.ad[sb]; do  \
		      g=`basename $$f | sed -e "s/^7\(.\)/\1-/" ` ; \
		      rm -f $$g ; \
		      ln -s $$f $$g > /dev/null 2>&1 || cp -p $$g $$g ; \
		   done;; \
	   esac ; \
	   rm -f ../stamp-gnatlib ;  \
	   touch ../stamp-gnatlib1)

gnatlib-common:
	(subdir=`cd $(srcdir); pwd`; \
	 cd rts ; \
	 $(MAKE)  CC="../../xgcc -B../../" \
                 INCLUDES="$(INCLUDES_FOR_SUBDIR) -I./../.." \
                 CFLAGS="$(GNATLIBCFLAGS)" \
	         -f ../Makefile libgnat.a ; \
	 $(MAKE)  CC="../../xgcc -B../../" \
	         ADAFLAGS="$(GNATLIBFLAGS)"\
                 CFLAGS="$(GNATLIBCFLAGS)" \
	         -f ../Makefile $(GNAT_RTL_COMMON_OBJS) ; \
	)

gnatlib: ../stamp-gnatlib1 ../stamp-gnatlib2 gnatlib-common
	(subdir=`cd $(srcdir); pwd`; \
	 cd rts ; \
	 case $(target) in \
		*-*-cygwin32*) \
		\
		 $(MAKE)  CC="../../xgcc -B../../" \
                        CFLAGS="$(GNATLIBCFLAGS)" \
	         	ADAFLAGS="$(GNATLIBFLAGS)"\
	         	-f ../Makefile $(GNAT_RTL_OLD_OBJS) ; \
		 $(MAKE)  CC="../../xgcc -B../../" \
                        CFLAGS="$(GNATLIBCFLAGS)" \
	         	ADAFLAGS="$(GNATLIBFLAGS)"\
	         	-f ../Makefile $(GNAT_RTL_WIN32_OBJS) ;; \
		hppa*-hp-hpux*         |\
		mips-sgi-irix*         |\
		alpha-dec-osf*)         \
		\
		 $(MAKE)  CC="../../xgcc -B../../" \
	         	ADAFLAGS="$(GNATLIBFLAGS)"\
                        CFLAGS="$(GNATLIBCFLAGS)" \
	         	-f ../Makefile $(GNAT_RTL_OLD_OBJS) ;; \
		sparc-sun-sunos4*      |\
		sparc-sun-solaris2*    |\
		sparc-sun-solaris5*    |\
		ppc-mac-machten        |\
		*-*-rtems* | *-rtems*  |\
		*-go32-msdos | *-go32  |\
                *86*-linux*            |\
                *)		       \
                \
		 $(MAKE)  CC="../../xgcc -B../../" \
	         	ADAFLAGS="$(GNATLIBFLAGS)"\
                        CFLAGS="$(GNATLIBCFLAGS)" \
         	-f ../Makefile $(GNAT_RTL_NEW_OBJS) ;; \
		\
	 esac ; \
	 if [ $$? -eq 0 ]; then touch ../stamp-gnatlib; fi \
	) 


