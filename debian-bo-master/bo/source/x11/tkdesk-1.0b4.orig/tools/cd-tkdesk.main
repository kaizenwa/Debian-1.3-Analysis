
wm withdraw .

if {[lsearch [winfo interps] "tkdesk"] < 0} {
    puts "Please start tkdesk first."
    exit 1
}

if {$argv == ""} {
    puts [send tkdesk dsk_active dir]
} else {
    if {$argv == "."} {
	send tkdesk dsk_active dir [pwd]
    } else {
	send tkdesk dsk_active dir [lindex $argv 0]
    }
}
exit 0
