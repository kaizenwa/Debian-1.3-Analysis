				GV_MovePage(start)	\
				GV_EraseLocator()	\
				GV_MiscMenu(init)	\n\
 Button3<Btn1Down>:	  	GV_MiscMenu(reset)	\n\
 Button3<Btn2Down>:	  	GV_MiscMenu(reset)	\n\
 ~Button1 ~Button2 <Btn3Motion>:GV_MovePage(move,0,1.0)\n\
 <Btn3Up>:			cursor(normal)		\
				GV_MiscMenu(popup)	\
				notify()		\n\
