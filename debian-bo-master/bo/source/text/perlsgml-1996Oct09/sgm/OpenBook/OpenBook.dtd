<!ELEMENT keycap    - -  (%sub-du1;)
                         -(needbegin | needend)                      >

<!-- Took out the newline inclusion since multiple lines cannot be 
     supported in softkeys.                                        -->
<!ELEMENT softkey   - -  (%sub-du1;)
                         -(needbegin | needend)                      >

<!ELEMENT user      - -  (%sub-du3;)                                 >

<!-- 
   - <esc> element has been taken out of the dtd since it no 
   - longer needs to be used in SGML editors.  Beta testing should 
   - verify this however.                                                 
  -->




<!-- ______ SPECIAL LAYOUT DATA ___________________-->

<!ELEMENT fillin    - O  EMPTY                                       >

<!-- Added the END attribute which was supported in HP Tag but was 
     not in the 1.22 DTD.                                          -->
<!ATTLIST fillin    length         NMTOKEN               #REQUIRED   
                    position       (END)                 #IMPLIED    >

<!ELEMENT leader    - O  EMPTY                                       >
<!ATTLIST leader    type           (DOTS | SPACE)        "DOTS"      >



<!-- ______ LINKED DATA ____________________________-->

<!-- Cross-references -->
<!ELEMENT xref      - O  EMPTY                                       >
<!ATTLIST xref      rid           IDREF                   #REQUIRED  >

<!-- Link
   -  The content of the link is part of the primary document flow.
   -
   -  May have a link inside of a link so keep recursion.           
   -  May have an id on a link and a reference to the link so we have 
   -      both ID and IDREF.                                            
   -
   - Link is similar to emphasis; it changes the contained info's rendering
   - and it adds a behavior. For print applications, link content is intended
   - to be printed unless the common_attr info overrides.
  -->
<!ELEMENT link      - -  (%sub-du;)                                 >
<!ATTLIST link           %conditionals;
                         %hyper-attr;                               >




<!-- ______ ASYNCHRONOUS DATA ____________________________-->


<!-- Have both text and partext content model because you may want to 
     encapsulate textual block or intra paragraph.                 -->

<!--  Need formatting elements to group text on a page.            -->
<!ELEMENT needbegin    - O  EMPTY                                    >
<!ELEMENT needend      - O  EMPTY                                    >
<!ATTLIST needbegin  %conditionals;                                  >

<!-- Revision bars - 1/11/93 dmh
   - revision regions have been replaced with a common attribute.
   -
   - Recursion is desirable with location. 
   - 1/11/93 dmh - But for now, locations is only a point in the 
   - document.
  -->
<!ELEMENT location  - -  (%sub-du;)                                  >
<!ATTLIST location       id   ID                          #IMPLIED   >






<!-- SPECIAL STRUCTURES -->

<!-- INDEX MARKUP -->
<!-- Index markup consists of the following major markup categories:
   - 1) Index user markup - markup used by users to create index entries
   -                        (primary and secondary) and to specify 
   -                        information about sorting and synonyms.
   - 2) Indexer-generated markup - markup used by the indexer program
   -                               to write the "generated index file"
   -                               which can be edited by users. 
  -->

<!-- _____________  Index User Markup __________________-->
<!ELEMENT idx         - -  (idxkey1, idxkey2?)
                          -(term | footnote | idx | 
                            needbegin | needend | fillin)   >
<!ATTLIST idx         main           (REF | MAIN)           "REF"
                      range          (BEGIN | END)          #IMPLIED   >
<!ELEMENT idxkey1     - -  ((%partext;), sort?)                        >
<!ELEMENT idxkey2     - -  ((%partext;), sort?)                        >
<!ELEMENT sort        - -  (#PCDATA)                                   >
<!--  Index synonyms list for "SEE" entries.  These elements were
   -  renamed from HP Tag to improve usability.                    
   -->
<!ELEMENT idxsyn       - O  (synonym+)
                           -(term | footnote | needbegin | needend | 
                            fillin | idx)                              >
<!ELEMENT synonym      - -  (seefromkey1, seefromkey2?, 
                          seetokey1, seetokey2?)                       >
<!ELEMENT seefromkey1  - - ((%partext;), sort?)                        >
<!ELEMENT seefromkey2  - - ((%partext;), sort?)                        >
<!ELEMENT seetokey1    - - ((%partext;), sort?)                        >
<!ELEMENT seetokey2    - - ((%partext;), sort?)                        >

<!-- Indexer-generated markup -->
<!ELEMENT index        O O  (indexsect+)
                           -(idx)                                      >
<!ELEMENT indexsect    O O  (indexletter, indexentry+)                 >
<!ELEMENT indexletter  O O  (%partext;)                                >
<!ELEMENT indexentry   O O  (indexkey1andref, indexkey2andref*)        >
<!ELEMENT indexkey1andref O O (indexkey1, (%indexrefs;)?)              >
<!ELEMENT indexkey2andref O O (indexkey2, (%indexrefs;))               >

<!ELEMENT indexkey1    O O  (%partext;)                                >
<!ELEMENT indexkey2    O O  (%partext;)                                >

<!ELEMENT indexchapref O O  (%partext;)                                >
<!ELEMENT indexchaprefmain  O O (%partext;)                            >
<!ELEMENT indexsyn     O O  (%partext;)                                >





<!-- ____ Formatting elements for page and line breaks. __________ -->

<!ELEMENT newline   - O  EMPTY                                       >
<!ELEMENT newpage   - O  EMPTY                                       >
<!ELEMENT nobreak   - -  (%partext;)
                        -(nobreak | needbegin | needend)             >
<!ATTLIST nobreak   id             ID                     #IMPLIED   >


