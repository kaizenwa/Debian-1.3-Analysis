<!-- _____________________ DATA UNIT ENTITIES __________________ -->

<!-- Intra-paragraph structures 
   -
   - These DATA UNITS contain all the actual text and data in
   - the document. Data Units are contained within paragraph level
   - information units described below.
  --> 

<!-- Classes of sub-paragraph data units
   -   These classes should not be used within elements
   -   content models. They are used to create the 
   -   collections in the next section.
  -->

<!-- Text that is used in building man-machine dialogs.  _________ -->
<!ENTITY % dialog   "(computer | cursor  | display | 
                      keycap   | softkey | user )"                   >

<!-- Text that requires special layout considerations. ___________ -->
<!ENTITY % sptext   "(footnote | ineqn | ingraphic | fillin)"        > 
<!ENTITY % spchars  "(super | sub)"                                  > 

<!-- Phrases of key significance. ________________________________ -->
<!ENTITY % keytext  "(acro  | book | emph  | location | not | 
                      parm  | term | var)"                           > 

<!-- DU level elements that contain other DU level information ___ -->
<!ENTITY % du-containers "(nobreak)"                                 >

<!-- Data that is linked to other data. __________________________ -->
<!ENTITY % links    "(link | xref)"                                  >





<!-- Collections of sub-paragraph data units.
   -   These collections are intended for use within the 
   -   element content models.
  -->

<!-- 
   - These entities are used as the content of many of the data units. 
  -->
<!ENTITY % sub-du1  "((#PCDATA | (%spchars;))*)"                    >
<!ENTITY % sub-du2  "((#PCDATA | keycap | var | ineqn | user |
                       cursor | super | sub)*)"                     >
<!ENTITY % sub-du3  "((#PCDATA | keycap | var | ineqn | 
                       super | sub)*)"                              >
<!ENTITY % sub-du4  "((#PCDATA | acro | keycap | link | nobreak | 
                       softkey | sub | super | xref)*)"             >

<!-- Broadest use for within data containers. ____________________ -->
<!ENTITY % sub-du   "((#PCDATA  | (%keytext;) | (%spchars;) |
                      (%links;) | (%dialog;)  | (%sptext;) | 
                      (%du-containers;))*)"                        > 

<!-- 
   - Syntax Diagram data containers 
  -->
<!ENTITY % synel  "(#PCDATA | var | ineqn | keycap |
                    optblock | reqblock | parm)"                     >




<!-- Simple sub-paragraph data units ____________________________ -->
<!ENTITY % simple-pt  "((#PCDATA | (%keytext))*)"                   >

<!-- Sub-paragraph data units; this is the same as partext in
   - the HP reference dtd. It is here to provide an easy way for
   - vendor specific applications to limit the content of 
   - particular containers with easier maintenance.
  -->
<!ENTITY % pt-lite   "((#PCDATA  | (%keytext;) | (%spchars;) |
                       (%links;) | (%dialog;)  | (%sptext;) | 
                       (%du-containers;))*)"                        > 

<!-- Complex sub-paragraph data units ___________________________ -->
<!ENTITY % partext   "((#PCDATA  | (%keytext;) | (%spchars;) |
                       (%links;) | (%dialog;)  | (%sptext;) | 
                       (%du-containers;))*)"                        > 








<!-- asynchronous markup used to identify regions of a document 
   -   This markup needs to be included in any content model where
   -   available. There is not dtd control to match begins and ends
   -   or to assure that a matching end exists. This is left to the
   -   application. 
   - These are included by inclusion at the section level.
  -->
<!ENTITY % async    "((needbegin | needend)*)"                       >




