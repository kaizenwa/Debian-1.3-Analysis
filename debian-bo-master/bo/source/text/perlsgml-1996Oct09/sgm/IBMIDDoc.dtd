                          "#ALL #ANY &Common.RefType;"
                        -- No REFLEVEL restrictions (see 10744 6.5.3) --
    refrange         CDATA        #FIXED    "#ALL I"   -- indirection --
    LexModel         CDATA        #FIXED
                          "RefID IDREFS &Common.LexModel;"
    HyNames          CDATA        #FIXED
