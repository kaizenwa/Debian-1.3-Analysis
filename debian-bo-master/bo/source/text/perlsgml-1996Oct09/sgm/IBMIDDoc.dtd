    ContentRef       NAME         #CONREF
    XRefText         CDATA        #IMPLIED
    InfoType         CDATA        #IMPLIED
    %Hyperlink.Attributes;
    %Property.Attributes;

    InfoMaster       NAME         #FIXED    "HyperLink"
    -- HyTime attributes: --
    HyTime           NAME         #FIXED    "ilink"
    HyNames          CDATA        #FIXED
                          "linkend LinkEnd &Common.HyNames;"
