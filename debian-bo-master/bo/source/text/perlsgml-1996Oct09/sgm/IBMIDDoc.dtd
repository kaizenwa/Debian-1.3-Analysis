<!ATTLIST  Frontmatter
    %Common.Atts;

    InfoMaster       NAME         #FIXED    "ContainingDivision"
>

<!ENTITY % Body             "Title?, (Part* | (Div | Procedure)*)" >

<!ELEMENT  Body             O O (%Body;) >
<!ATTLIST  Body
    %Common.Atts;

    InfoMaster       NAME         #FIXED    "ContainingDivision"
>

