# Makefile for Borland C++ 2.0
# Place your library directory below

LIB=c:\prog\c\lib
MDL=c

mgt.exe: ascii.obj build.obj comment.obj doit.obj edit.obj\
         parse.obj play.obj tree.obj mgt.obj help.obj mou.obj
    tlink /c /x $(LIB)\c0$(MDL) $(LIB)\wildargs ascii build comment doit \
          edit mgt parse play tree mou help ,mgt,mgt,\
          $(LIB)\c$(MDL) 

ascii.obj: asc_ibm.inc ascii.c

.c.obj: 
    bccx -m$(MDL) -w-pia -G -Z -O -c -DMGT_IBM $*.c



