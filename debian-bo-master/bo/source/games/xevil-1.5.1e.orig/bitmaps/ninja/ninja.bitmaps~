Size Ninja::get_size_max() 
{
  Size ret;
  ret.width = 25;
  ret.height = 27;
  return ret;
}



PhysicalP Ninja::create(WorldP w,LocatorP l,const Pos &pos)
{
  PhysicalP ret = new Ninja(w,l,pos);
  assert(ret);
  return ret;
}



