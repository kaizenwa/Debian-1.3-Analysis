// Defines
#define ALIEN_DAMAGE 23


Size Alien::get_size_max() 
{
  Size ret;
  ret.width = 30;
  ret.height = 29;
  return ret;
}



PhysicalP Alien::create(WorldP w,LocatorP l,const Pos &pos)
{
  PhysicalP ret = new Alien(w,l,pos);
  assert(ret);
  return ret;
}



