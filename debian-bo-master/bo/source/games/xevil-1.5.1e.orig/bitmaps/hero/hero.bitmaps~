#define HERO_HEAL_MULTIPLIER 2.0
#define HERO_HEAL 1
#define HERO_HEAL_TIME 4
  
Size Hero::get_size_max() 
{
  Size ret;
  ret.width = 31;
  ret.height = 31;
  return ret;
}



PhysicalP Hero::create(WorldP w,LocatorP l,const Pos &pos)
{
  PhysicalP ret = new Hero(w,l,pos);
  assert(ret);
  return ret;
}



