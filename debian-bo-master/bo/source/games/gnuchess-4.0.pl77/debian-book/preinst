#! /bin/bash
# Pre-installation script for gnuchess-book
# Nikhil Nair <nnair@debian.org>, 12th August 1996

if [ "$1" != install ]
then
  exit 0;
fi

dpkg-divert --rename --quiet \
  --divert /usr/lib/games/gnuchess/gnuchess.data.small \
  --package gnuchess-book /usr/lib/games/gnuchess/gnuchess.data
