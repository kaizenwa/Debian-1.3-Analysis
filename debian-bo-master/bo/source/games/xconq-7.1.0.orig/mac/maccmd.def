/* The definitions of Mac-specific commands. */

DEF_CMD(C('['), "escape", "", do_mac_escape,
  "")

DEF_CMD(  '@' , "angle", "", do_mac_set_map_angle,
  "change the angle at which a map is viewed")

DEF_CMD(  '{' , "zoom-out", "", do_mac_zoom_out,
  "zoom out to see less map detail")

DEF_CMD(  '}' , "zoom-in", "", do_mac_zoom_in,
  "zoom in to see more map detail")

#ifdef DEBUGGING

DEF_CMD(   0  , "DP", "", do_profile,
  "")

DEF_CMD(   0  , "DT", "", do_trace,
  "")

#endif

