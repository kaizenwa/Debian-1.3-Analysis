# Curses Imakefile.

CDEBUGFLAGS = -g

GLIBDIR = ../lib

DEFINES = -DUNIX -DXCONQLIB=\"$(GLIBDIR)\"\"

INCLUDES = -I../x11 -I../kernel

LOCAL_LIBRARIES = -lcurses -ltermcap

CONQ_LIBRARIES = ../kernel/libconq.a ../kernel/libconqlow.a

SRCS1 = cconq.c

OBJS1 = cconq.o \
	ccmd.o \
	cdraw.o

PROGRAMS = cconq

ComplexProgramTarget_1(cconq, $(CONQ_LIBRARIES), $(LOCAL_LIBRARIES))
