	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) lifeconv.man $(DESTDIR)$(MANDIR)/lifeconv.$(MANSUFFIX)

