 srcdir = @srcdir@
  VPATH = @srcdir@
  SHELL = /bin/sh
SUBDIRS = V src objects

# This line is needed to force BSD-style makes to do the right target
# by default -- they get confused by the following target.
defaulttarget: all

all install uninstall clean mostlyclean realclean tags info dvi dist check depend:
	@for i in $(SUBDIRS) ; do \
		echo Making $@ in ./$${i} ... ; \
		( cd $$i ; $(MAKE) $(MFLAGS) $@ ) ; \
	done

Makefile: Makefile.in config.status
	$(SHELL) config.status

config.status:
	$(SHELL) $(srcdir)/configure --no-create

configure: configure.in
	cd $(srcdir); autoconf

distclean:
	@for i in $(SUBDIRS) ; do \
		echo Making $@ in ./$${i} ... ; \
		( cd $$i ; $(MAKE) $(MFLAGS) $@ ) ; \
	done
	rm -f Makefile config.status
