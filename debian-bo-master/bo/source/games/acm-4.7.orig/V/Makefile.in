  SHELL = /bin/sh
SUBDIRS = lib

# If you wish to debug or test Vlib, uncomment this definition ...
#SUBDIRS = lib test

defaulttarget: all

all install uninstall clean mostlyclean realclean tags info dvi dist check depend:
	@for i in $(SUBDIRS) ; do \
		echo Making $@ in ./$${i} ... ; \
		( cd $$i ; $(MAKE) $(MFLAGS) $@ ) ; \
	done

distclean:
	@for i in $(SUBDIRS) ; do \
		echo Making $@ in ./$${i} ... ; \
		( cd $$i ; $(MAKE) $(MFLAGS) $@ ) ; \
	done
	rm -f Makefile
