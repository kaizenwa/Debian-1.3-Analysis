	if test "$random" = "1" ; then
		DEFS="$DEFS -DHAVE_RANDOM"
	else
		echo "Hmm. Your system does not support either random() or rand()."
		echo "ACM needs one of the random number generators to operate."
		exit 1
	fi
fi

