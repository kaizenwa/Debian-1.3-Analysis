install.man:: xmille.man
	 @if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xmille.man $(DESTDIR)$(MANDIR)/xmille.$(MANSUFFIX)

