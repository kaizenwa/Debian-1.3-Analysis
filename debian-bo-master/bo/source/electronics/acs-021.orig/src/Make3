#$Id: Make3,v 11.22 96/02/18 11:41:34 al Exp $
# Part 3 of a Makefile
#------------------------------------------------------------------------
acs: $(OBJS) Makefile
	-rm $@ >/dev/null 2>&1
	$(CCC) $(CCFLAGS) $(OBJS) -o $@ $(LIBS) $(LDFLAGS)
#------------------------------------------------------------------------
tags: $(SRCS) $(HDRS)
	ctags -d $(SRCS) $(HDRS)
#------------------------------------------------------------------------
unclean:
	rm $(SRCS) $(HDRS) $(OTHER)
#------------------------------------------------------------------------
backup:
	-mkdir BACKUP
	cp $(SRCS) $(HDRS) $(OTHER) BACKUP
#------------------------------------------------------------------------
clean:
	-rm *.o
#------------------------------------------------------------------------
depend: $(SRCS)
	$(CCC) -MM $(CCFLAGS) $(SRCS) > Make.depend
#-----------------------------------------------------------------------------
#-----------------------------------------------------------------------------
