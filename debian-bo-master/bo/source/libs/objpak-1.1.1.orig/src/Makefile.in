SUBDIRS = ObjCltn ObjDic ObjPak ObjSet ObjSeq ObjSort ObjStr

objects::
	@for i in $(SUBDIRS) nonsubdirs ; do \
		if [ $$i = nonsubdirs ] ; then continue; fi; \
		(cd $$i; ${MAKE} objects) \
	done;

debug::
	@for i in $(SUBDIRS) nonsubdirs ; do \
		if [ $$i = nonsubdirs ] ; then continue; fi; \
		(cd $$i; ${MAKE} debug) \
	done;

clean::
	@for i in $(SUBDIRS) nonsubdirs ; do \
		if [ $$i = nonsubdirs ] ; then continue; fi; \
		(cd $$i; ${MAKE} clean) \
	done;

distclean::
	@for i in $(SUBDIRS) nonsubdirs ; do \
		if [ $$i = nonsubdirs ] ; then continue; fi; \
		(cd $$i; ${MAKE} distclean) \
	done;

