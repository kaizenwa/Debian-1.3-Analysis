# ObjectPak Objective C Class Library
#
###############################################################################

libdir = @libdir@

objects::
	rm -f ${libdir}/libobjpak.a
	cd src;${MAKE} objects
	-ranlib ${libdir}/libobjpak.a

debug::
	rm -f ${libdir}/libobjpak.a
	cd src;${MAKE} debug
	-ranlib ${libdir}/libobjpak.a

clean ::
	cd src;${MAKE} clean
	cd words;${MAKE} clean

distclean ::
	cd src;${MAKE} distclean
	cd words;${MAKE} distclean

