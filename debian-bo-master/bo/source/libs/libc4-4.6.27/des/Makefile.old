OPTS= -DALT_ECB

CFLAGS = -I. -O2 -m486 $(OPTS)

OBJS = des_crypt.o des_soft.o des_impl.o

all:	libdes.a destest speed

destest: destest.o libdes.a
	$(CC) $(CFLAGS) -o destest destest.o libdes.a

speed: speed.o libdes.a
	$(CC) $(CFLAGS) -o speed speed.o libdes.a


libdes.a: ${OBJS}
	ar cru libdes.a ${OBJS}
	ranlib libdes.a

clean:
	rm -f *.o libdes.a destest speed
